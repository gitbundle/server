{
  "name": "magit-server",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">= 14"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "NODE_ENV=development webpack-dev-server --progress",
    "dev:nodemon": "nodemon --watch webpack.config.js --exec 'NODE_ENV=development webpack-dev-server --progress'",
    "dev:raw": "nodemon --watch webpack.config.js --exec 'NODE_ENV=development webpack --watch --progress'",
    "dev:livereload": "npx livereload -d -w 5 -e html,css,js,ts,svg,png,svelte templates,web_src",
    "dev:debug": "onchange '**' -- node -e 'console.log(\"{{changed}}\")'",
    "dev:prettierd": "onchange '**' --exclude-path .prettierignore -- prettier --write --ignore-unknown {{changed}}",
    "dev:eslintd": "onchange '**' --exclude-path .gitignore -- npx eslint --ext .js --ext .ts --ext .css --ext .html --fix {{changed}}",
    "dev:beta": "nodemon --watch webpack.config.js --exec 'NODE_ENV=beta webpack-dev-server --progress'",
    "clean": "make clean-frontend",
    "build": "npm run compile",
    "compile": "NODE_ENV=production webpack --progress",
    "compile:beta": "NODE_ENV=beta webpack --progress",
    "lint": "prettier --check ./**/*.{html,svelte,js,css,ts} && eslint ./**/*.{html,svelte,js,css,ts}",
    "format": "prettier --plugin prettier-plugin-svelte --plugin prettier-plugin-tailwindcss --write ./**/*.{html,svelte,js,css,ts}"
  },
  "browserslist": [
    "defaults",
    "not ie > 0",
    "not ie_mob > 0"
  ],
  "author": "",
  "devDependencies": {
    "@skeletonlabs/skeleton": "1.11.0",
    "@sveltejs/vite-plugin-svelte": "2.0.4",
    "@typescript-eslint/eslint-plugin": "5.58.0",
    "@typescript-eslint/parser": "5.58.0",
    "add-asset-webpack-plugin": "2.0.1",
    "asciinema-player": "3.2.0",
    "autoprefixer": "10.4.14",
    "colortranslator": "1.10.2",
    "css-loader": "6.7.3",
    "esbuild-loader": "3.0.1",
    "esbuild-plugin-less": "1.1.13",
    "eslint": "8.38.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jquery": "1.5.1",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-sonarjs": "0.19.0",
    "eslint-plugin-svelte3": "4.0.0",
